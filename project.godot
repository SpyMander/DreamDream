config_version=5

[application]

config/name="40 Dream Dream"
run/main_scene="res://scenes/levels/testLevel.tscn"
config/features=PackedStringArray("4.0", "C#", "Forward Plus")
run/max_fps=300
config/icon="res://icon.svg"

[autoload]

InteractableObserver="res://scripts/observers/InteractableObserver.cs"
